#!/bin/bash

set -e
set -o pipefail

driver_id=${1-1}

commands=(
  cartesian-to-polar
  homebody-polar-coordinate
  homebody-distance
  calculate-homebody-rotation
  rotate-homebody-angles
  convert-rotation-to-cartesian
  width
)

for command in ${commands[@]}
do
    echo "$command"
    bin/morphology/$command "$driver_id"
done
